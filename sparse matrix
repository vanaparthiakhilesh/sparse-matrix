#include<stdio.h>
int main()
{
int a[100][100],row,cols;
int i,j;
printf("Enter the number of rows :");
scanf("%d",&row);
printf("Enter the number of columns :");
scanf("%d",&cols);
printf("Enter the array elements:");
for(i=0;i<row;i++)
for(j=0;j<cols;j++)
scanf("%d",&a[i][j]);
printf("Required elements :");
for(i=0;i<row;i++)
{
for(j=0;j<cols;j++)
{
    if(a[i][j]!=0)
printf("%d",a[i][j]);
}
}
}
